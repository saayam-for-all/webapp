{
  "name": "@mui/utils",
  "version": "7.0.2",
  "private": false,
  "author": "MUI Team",
  "description": "Utility functions for React components.",
  "main": "./index.js",
  "keywords": [
    "react",
    "react-component",
    "mui",
    "utils"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mui/material-ui.git",
    "directory": "packages/mui-utils"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mui/material-ui/issues"
  },
  "homepage": "https://github.com/mui/material-ui/tree/master/packages/mui-utils",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/mui-org"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "@types/prop-types": "^15.7.14",
    "clsx": "^2.1.1",
    "prop-types": "^15.8.1",
    "react-is": "^19.1.0",
    "@mui/types": "^7.4.1"
  },
  "peerDependencies": {
    "@types/react": "^17.0.0 || ^18.0.0 || ^19.0.0",
    "react": "^17.0.0 || ^18.0.0 || ^19.0.0"
  },
  "peerDependenciesMeta": {
    "@types/react": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "directory": "build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./*/index.d.ts",
        "default": "./*/index.js"
      },
      "import": {
        "types": "./esm/*/index.d.ts",
        "default": "./esm/*/index.js"
      }
    },
    "./esm": null,
    "./modern": null
  },
  "types": "./index.d.ts"
}